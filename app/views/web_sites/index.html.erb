<h1>Listing web_sites</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @web_sites.each do |web_site| %>
  <tr>
    <td><%=h web_site.url %></td>
    <td><%=h web_site.name %></td>
    <td><%=h web_site.description %></td>
    <td><%= link_to 'Show', web_site %></td>
    <td><%= link_to 'Edit', edit_web_site_path(web_site) %></td>
    <td><%= link_to 'Destroy', web_site, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New web_site', new_web_site_path %>